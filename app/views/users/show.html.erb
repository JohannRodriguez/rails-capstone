<!DOCTYPE html>
<html>
  <body>
    <section>
      <% @user.articles.order('created_at').reverse_order.each do |article| %>
      <article class="">
        <h3><%= link_to article.title, article_path(:id => article.id) %></h3>
        <img src="<%= article.image %>" width="150px" onerror="this.onerror=null; this.src='https://farm5.staticflickr.com/4363/36346283311_1dec5bb2c2.jpg'">
        <p><%= article.votes.length %> <%= user_index_votes(article) %></p>
      </article>
      <% end %>
    </section>
  </body>
</html>
